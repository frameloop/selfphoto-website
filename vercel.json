{
  "version": 2,
  "buildCommand": "echo 'No build required'",
  "outputDirectory": "web",
  "builds": [
    {
      "src": "web/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/web/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/web/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/web/images/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/web/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/web/server.js"
    }
  ]
}
