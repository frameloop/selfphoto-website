{
    "version": 2,
    "builds": [
        {
            "src": "web/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/web/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ]
}